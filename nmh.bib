@Article{	  AguilarGoodman:06,
  title		= "Anisotropic mesh refinement for finite element methods
		  based on error reduction",
  journal	= "Journal of Computational and Applied Mathematics",
  volume	= "193",
  number	= "2",
  pages		= "497 - 515",
  year		= "2006",
  issn		= "0377-0427",
  doi		= "https://doi.org/10.1016/j.cam.2005.05.036",
  url		= "http://www.sciencedirect.com/science/article/pii/S0377042705004280",
  author	= "Juan C. Aguilar and Jonathan B. Goodman",
  keywords	= "Finite elements, Adaptive mesh refinement, Anisotropic
		  refinement, Triangular grids, Error estimators",
  abstract	= "In this paper, we propose an anisotropic adaptive
		  refinement algorithm based on the finite element methods
		  for the numerical solution of partial differential
		  equations. In 2-D, for a given triangular grid and finite
		  element approximating space V, we obtain information on
		  location and direction of refinement by estimating the
		  reduction of the error if a single degree of freedom is
		  added to V. For our model problem the algorithm fits highly
		  stretched triangles along an interior layer, reducing the
		  number of degrees of freedom that a standard h-type
		  isotropic refinement algorithm would use."
}

